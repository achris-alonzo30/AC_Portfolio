{
  "version": 2,
  "builds": [
    {
      "src": "AC_Portfolio/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",  // Match static files
      "dest": "website/static/$1"  // Map to the actual location of static files
    },
    {
      "src": "/(.*)",
      "dest": "AC_Portfolio/wsgi.py"
    }
  ]
}
